<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:background="@drawable/background"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:scrollbars="vertical"
    tools:context=".MainActivity4">

    <ScrollView
        android:id="@+id/scrollView"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:isScrollContainer="false"
        tools:layout_editor_absoluteX="0dp"
        tools:layout_editor_absoluteY="100dp">

        <LinearLayout
            android:id="@+id/LinearLayout"
            android:layout_width="fill_parent"
            android:layout_height="fill_parent"
            android:orientation="vertical">

            <TextView
                android:text="アンケート"
                android:id="@+id/title"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:gravity="center"
                android:padding="5dp"
                android:textColorLink="#FFFFFF"
                android:textSize="34sp"
                app:layout_constraintLeft_toLeftOf="parent"
                app:layout_constraintRight_toRightOf="parent"
                app:layout_constraintTop_toTopOf="parent" />

            <TextView
                android:id="@+id/comment"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:padding="5dp"
                android:text="以下の質問にお答えください。あなたにぴったりの物件をお勧めします。"
                android:textSize="14sp"
                app:layout_constraintLeft_toLeftOf="parent"
                app:layout_constraintRight_toRightOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/title" />

            <TextView
                android:text="氏名"
                android:id="@+id/subtitle7"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:paddingLeft="5dp"
                android:paddingTop="5dp"
                android:textSize="16sp"
                android:textStyle="bold"
                app:layout_constraintHorizontal_bias="0.0"
                app:layout_constraintLeft_toLeftOf="parent"
                app:layout_constraintRight_toRightOf="parent"
                app:layout_constraintTop_toBottomOf="@id/comment" />

            <EditText
                android:id="@+id/editTextTextPersonName11"
                android:layout_width="322dp"
                android:layout_height="44dp"
                android:layout_margin="10dp"
                android:background="@drawable/edittext_flame1"
                android:ems="10"
                android:hint="琉球太郎"
                android:inputType="textPersonName"
                app:backgroundTint="#FFFFFFFF"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0.419"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/subtitle7"
                app:layout_constraintVertical_bias="0.665" />

            <TextView
                android:text="年齢"
                android:paddingTop="5dp"
                android:paddingLeft="5dp"
                android:textSize="16sp"
                android:textStyle="bold"
                android:id="@+id/subtitle8"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                app:layout_constraintLeft_toLeftOf="parent"
                app:layout_constraintRight_toRightOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/editTextTextPersonName11"
                />

            <EditText
                android:id="@+id/editTextTextPersonName12"
                android:layout_width="322dp"
                android:layout_height="44dp"
                android:layout_margin="10dp"
                android:background="@drawable/edittext_flame1"
                android:ems="10"
                android:hint="21"
                android:inputType="number"
                app:backgroundTint="#FFFFFFFF"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0.419"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/subtitle8"
                app:layout_constraintVertical_bias="0.757" />


            <TextView
                android:text="性別"
                android:id="@+id/subtitle9"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:paddingLeft="5dp"
                android:paddingTop="5dp"
                android:textSize="16sp"
                android:textStyle="bold"
                app:layout_constraintHorizontal_bias="0.0"
                app:layout_constraintLeft_toLeftOf="parent"
                app:layout_constraintRight_toRightOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/editTextTextPersonName12" />

            <Spinner
                android:id="@+id/editTextTextPersonName13"
                android:layout_width="322dp"
                android:layout_height="44dp"
                android:layout_margin="10dp"
                android:background="@drawable/edittext_flame1"
                android:ems="10"
                app:backgroundTint="#FFFFFFFF"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0.419"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/subtitle9"
                app:layout_constraintVertical_bias="0.846" />



        <TextView
            android:text="現住所"
            android:paddingTop="5dp"
            android:paddingLeft="5dp"
            android:textSize="16sp"
            android:textStyle="bold"
            android:id="@+id/subtitle1"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintRight_toRightOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/editTextTextPersonName13"
            />

            <EditText
                android:id="@+id/editTextTextPersonName7"
                android:layout_width="322dp"
                android:layout_height="44dp"
                android:layout_margin="10dp"
                android:background="@drawable/edittext_flame1"
                android:ems="10"
                android:inputType="textShortMessage"
                android:paddingTop="5dp"
                android:singleLine="false"
                app:backgroundTint="#FFFFFFFF"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/subtitle1"
                app:layout_constraintVertical_bias="0.046" />


        <TextView
            android:text="職業"
            android:paddingTop="5dp"
            android:paddingLeft="5dp"
            android:textSize="16sp"
            android:textStyle="bold"
            android:id="@+id/subtitle2"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintRight_toRightOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/editTextTextPersonName7"
            />

        <EditText
            android:id="@+id/editTextTextPersonName5"
            android:layout_width="322dp"
            android:layout_height="44dp"
            android:layout_margin="10dp"
            android:ems="10"
            android:inputType="textPersonName"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.495"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/subtitle2"
            android:background="@drawable/edittext_flame1"
            app:backgroundTint="#FFFFFFFF"
            app:layout_constraintVertical_bias="0.148" />

        <TextView
            android:text="職場の住所"
            android:paddingTop="5dp"
            android:paddingLeft="5dp"
            android:textSize="16sp"
            android:textStyle="bold"
            android:id="@+id/subtitle3"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintRight_toRightOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/editTextTextPersonName5"
            />

        <EditText
            android:id="@+id/editTextTextPersonName6"
            android:layout_width="322dp"
            android:layout_height="44dp"
            android:layout_margin="10dp"
            android:ems="10"
            android:inputType="textPersonName"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.437"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/subtitle3"
            android:background="@drawable/edittext_flame1"
            app:backgroundTint="#FFFFFFFF"
            app:layout_constraintVertical_bias="0.238" />

        <TextView
            android:text="家賃の希望額"
            android:paddingTop="5dp"
            android:paddingLeft="5dp"
            android:textSize="16sp"
            android:textStyle="bold"
            android:id="@+id/subtitle4"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintRight_toRightOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/editTextTextPersonName6"
            />

        <EditText
            android:id="@+id/editTextTextPersonName8"
            android:layout_width="322dp"
            android:layout_height="44dp"
            android:layout_margin="10dp"
            android:ems="10"
            android:inputType="textPersonName"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.454"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/subtitle4"
            android:background="@drawable/edittext_flame1"
            app:backgroundTint="#FFFFFFFF"
            app:layout_constraintVertical_bias="0.339" />

        <TextView
            android:text="何人家族か"
            android:paddingTop="5dp"
            android:paddingLeft="5dp"
            android:textSize="16sp"
            android:textStyle="bold"
            android:id="@+id/subtitle5"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintRight_toRightOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/editTextTextPersonName8"
            />

        <EditText
            android:id="@+id/editTextTextPersonName9"
            android:layout_width="322dp"
            android:layout_height="44dp"
            android:layout_margin="10dp"
            android:ems="10"
            android:inputType="textPersonName"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.496"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/subtitle5"
            android:background="@drawable/edittext_flame1"
            app:backgroundTint="#FFFFFFFF"
            app:layout_constraintVertical_bias="0.439" />

        <TextView
            android:text="その他の希望"
            android:paddingTop="5dp"
            android:paddingLeft="5dp"
            android:textSize="16sp"
            android:textStyle="bold"
            android:id="@+id/subtitle6"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintRight_toRightOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/editTextTextPersonName9"
            />

        <EditText
            android:id="@+id/editTextTextPersonName10"
            android:layout_width="322dp"
            android:layout_height="88dp"
            android:layout_margin="10dp"
            android:ems="10"
            android:inputType="textPersonName"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.419"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/subtitle6"
            android:background="@drawable/edittext_flame1"
            app:backgroundTint="#FFFFFFFF"
            app:layout_constraintVertical_bias="0.528" />



            <Button
                android:id="@+id/button5"
                android:layout_width="136dp"
                android:layout_height="58dp"
                android:enabled="true"
                android:text="送信"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0.498"
                app:layout_constraintStart_toStartOf="parent" />

        </LinearLayout>
    </ScrollView>


</androidx.constraintlayout.widget.ConstraintLayout>